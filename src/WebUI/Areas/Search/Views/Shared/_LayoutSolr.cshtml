<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>Bootstrap, from Twitter</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">

    <!-- Styles -->
    @Styles.Render("~/bundles/css")
    <style type="text/css">
        body
        {
            padding-top: 60px;
            padding-bottom: 40px;
        }

        ul#languages
        {
            font-size: 12px;
            margin: 0 0 5px;
            padding: 0;
            text-align: right;
        }

            ul#languages li
            {
                display: inline;
                list-style: none;
                padding-left: 15px;
            }

                ul#languages li a
                {
                    background: none;
                    color: #999;
                    text-decoration: none;
                }

                    ul#languages li a:hover
                    {
                        color: #333;
                        text-decoration: none;
                    }
    </style>
    @Scripts.Render("~/bundles/modernizr")

    <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script src="../assets/js/html5shiv.js"></script>
    <![endif]-->

</head>
<body>



    @Scripts.Render("~/bundles/jquery")


    <div class="container">
        @RenderBody()
    </div>

    @Scripts.Render("~/bundles/jqueryvals")

</body>

</html>
